{
  "name": "tlapnet/report",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tlapnet",
      "email": "webmaster@tlapnet.cz"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.ispalliance.cz/"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "ninjify/qa": "^0.3.3",
    "phpunit/phpunit": "^5.3.0",
    "nette/application": "^2.4.5",
    "nette/di": "^2.4.7",
    "nette/database": "^2.4.2",
    "nette/neon": "^2.4.1"
  },
  "suggest": {
    "dibi/dibi": "Allows use DibiDataSource",
    "nette/di": "Allows use this library as Nette extension",
    "nette/application": "Allows use Components",
    "nette/database": "Allows use NetteDatabaseDataSource",
    "nette/neon": "Allows use NeonLoader"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tlapnet\\Report\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "qa": [
      "linter src",
      "codesniffer src"
    ],
    "phpunit": [
      "phpunit --colors=always -c phpunit.xml"
    ],
    "phpstan": [
      "phpstan analyze --level 5 src tests"
    ]
  }
}
