{
  "name": "tlapnet/report",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tlapnet",
      "email": "webmaster@tlapnet.cz"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.ispalliance.cz/"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "ninjify/qa": "^0.3.3",
    "phpunit/phpunit": "^5.3.0",
    "tlapnet/chart": "dev-master",
    "dibi/dibi": "^2.3.5",
    "nette/di": "^2.4.7",
    "nette/application": "^2.4.5",
    "nette/forms": "^2.4.3",
    "nette/database": "^2.4.2",
    "nette/neon": "^2.4.1",
    "tracy/tracy": "^2.4.6"
  },
  "suggest": {
    "dibi/dibi": "Allows use Dibi\\DataSource",
    "tlapnet/chart": "Allows use Charts\\Renderers",
    "nette/di": "Allows use Nette\\DI [Extensions]",
    "nette/application": "Allows use Components",
    "nette/forms": "Allows use Nette\\Components [Controls]",
    "nette/database": "Allows use Nette\\Database [DataSources]",
    "nette/neon": "Allows use Neon\\Loaders",
    "tracy/tracy": "Allows use Connections & DataSources panels"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Report\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tlapnet\\Report\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "scripts": {
    "qa": [
      "linter src tests",
      "codesniffer src tests"
    ],
    "phpunit": [
      "phpunit --colors=always -c phpunit.xml"
    ],
    "phpstan": [
      "phpstan analyze --level 1 src tests"
    ]
  }
}
